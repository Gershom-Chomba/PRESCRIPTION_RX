@model IEnumerable<PRESCRIPTIONS_RX.Models.Contract>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Startdate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Enddate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContractText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PharmaceuticalNameNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PharmacyNameNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Startdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enddate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContractText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PharmaceuticalNameNavigation.CompanyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PharmacyNameNavigation.PharmacyName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ContractId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ContractId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ContractId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
